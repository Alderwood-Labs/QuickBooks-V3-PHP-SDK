{
    "name": "wssi/quickbooks-v3-php-sdk",
    "description": "A Custom Version of the PHP SDK for QuickBooks Online Accounting API",
    "type": "library",
    "keywords": ["api", "http", "rest", "quickbooks", "smallbusiness"],
    "homepage": "http://developer.intuit.com",
    "require": {
        "php": ">=5.6.0",
        "ext-mbstring": "*",
        "ext-dom": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "QuickBooksOnline\\API\\": "src/"
        }
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Cory Vernon",
            "email": "cory@snapdsk.com"
        }
    ],
    "suggest": {
        "ext-curl": "Uses Curl to make HTTP Requests",
        "guzzlehttp/guzzle": "Uses Guzzle to make HTTP Requests"
    }
}
